<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.going.grade.persistence.GradeMapper">

	<select id="selectBasic" resultType="org.going.grade.domain.GrademgmtVO">
		select * from grademgmt where customergrade='basic'
	</select>
	
	<insert id="createBasic">
		insert into grademgmt values ('basic', 0)
	</insert>
</mapper>